create table problems(
       id int NOT NULL AUTO_INCREMENT, 
       title varchar(255), 
       description varchar(255),
       coordinate_x int,
       coordinate_y int,
       created_by int,
       status int,
       created_at datetime,
       updated_at datetime,
       updated_by int,
       PRIMARY KEY (id) 
);


